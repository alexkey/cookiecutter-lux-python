{
    "name": "Foo Python Package",
    "repo_name": "{{ cookiecutter.name | lower | replace(' ', '_') | replace('.', '_') }}",
    "python_version": "3.5",
    "python": "python{{ cookiecutter.python_version }}",
    "pip_list_format": "columns",
    "virtualenv_dir_name": ".venv",
    "shebang_coding": "# -*- coding: utf-8 -*-",
    "shebang_shell": "#!/bin/bash",
    "shebang_python": "#!/usr/bin/env python",
    "vagrant_name": "Foo",
    "vagrant_box": "debian/jessie64",
    "vagrant_hostname": "foo",
    "forwarded_port_guest": "80",
    "forwarded_port_host": "8080",
    "vagrant_ip": "10.1.1.101",
    "vagrant_cpus": "2",
    "vagrant_memsz": "2048",
    "vagrant_vramsz": "16",
    "vagrant_accel3d": "off",
    "vagrant_provision": "script/bootstrap.sh",
    "vagrant_shell_source": "",
    "vagrant_roles": "all",
    "vagrant_ansible_verbose": "false",
    "vagrant_shared": ".",
    "vagrant_shared_guest": "/var/shared",
    "vagrant_gui": "false",
    "nodejs_version": "",
    "redis_version": "",
    "redis_port": "{{ '6379' if cookiecutter.redis_version else 'n/a' }}",
    "redis_config_file": "{{ '/etc/redis/%s.conf' % cookiecutter.redis_port if cookiecutter.redis_version else 'n/a' }}",
    "redis_log_file": "{{ '/var/log/redis_%s.log' % cookiecutter.redis_port if cookiecutter.redis_version else 'n/a' }}",
    "redis_data_dir": "{{ '/var/lib/redis/%s' % cookiecutter.redis_port if cookiecutter.redis_version else 'n/a' }}",
    "redis_executable": "{{ '/usr/local/bin/redis-server' if cookiecutter.redis_version else 'n/a' }}"
}
